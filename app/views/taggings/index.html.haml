- title "Taggings"

%table
  %tr
    %th Contact
    %th Asset
    %th Asset type
  - for tagging in @taggings
    %tr
      %td= h tagging.contact_id
      %td= h tagging.asset_id
      %td= h tagging.asset_type
      %td= link_to 'Show', tagging
      %td= link_to 'Edit', edit_tagging_path(tagging)
      %td= link_to 'Destroy', tagging, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Tagging", new_tagging_path
